  <builders>
    <hudson.tasks.Shell>
      <command>echo &quot;cluster id is ${clusterId}&quot; 

# Get MS IP
MS_IP=`wget -q -O - --no-check-certificate &quot;https://cifwk-oss.lmera.ericsson.se/generateTAFHostPropertiesJSON/?clusterId=${clusterId}&amp;tunnel=true&quot; | awk -F&apos;,&apos; &apos;{print $1}&apos; | awk -F&apos;:&apos; &apos;{print $2}&apos; | sed -e &quot;s/\&quot;//g&quot; -e &quot;s/ //g&quot;`

jenkins/slave/job_setup.sh $MS_IP

echo &quot;ip address from cluster id ${clusterId} is $MS_IP&quot;

ssh root@${MS_IP} &quot;cd /root/rvb/;sh deployment_setup.sh ${clusterId}&quot;</command>
    </hudson.tasks.Shell>
